<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9FCFF"
    android:gravity="center_horizontal"
    tools:context=".RatingPage">

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.google.android.material.textfield.TextInputLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="379dp"
        android:orientation="horizontal"></LinearLayout>

    <RatingBar
        android:id="@+id/ratingBar2"
        android:layout_width="257dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="77dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="428dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#02FF8A65"
        android:numStars="5"
        android:progressBackgroundTint="#4A636062"
        android:progressTint="#FBA21F" />

    <EditText
        android:id="@+id/editTextTextMultiLine2"
        android:layout_width="336dp"
        android:layout_height="102dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="38dp"
        android:layout_marginLeft="30dp"
        android:layout_marginEnd="37dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="303dp"
        android:background="#85C6C9CC"
        android:backgroundTint="#85C6C9CC"
        android:ems="15"
        android:gravity="center|start|top"
        android:hint="Enter your feedback"
        android:inputType="textMultiLine"
        android:lines="4" />

    <Button
        android:id="@+id/button3"
        android:layout_width="216dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="97dp"
        android:layout_marginEnd="98dp"
        android:layout_marginBottom="238dp"
        android:text="Isubmit ang sagot" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="366dp"
        android:layout_height="322dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="353dp"
        app:srcCompat="@drawable/gamauth" />


</RelativeLayout>